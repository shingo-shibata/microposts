<div class="row">
  <aside class="col-md-10">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %><br>
        <%= @title %>
      </h1>
    </section>
  </aside>
  <div class="col-md-10">
    <ol><%= render @users %></ol>
    <% @users.each do |user| %>
        <tr>
          <td>Name</td>
          <td>Region</td>
        </tr>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.region %></td>
        </tr>
    <% end %>
  </div>
</div>